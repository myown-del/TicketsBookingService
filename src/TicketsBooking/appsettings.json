{
  "Application": {},
  "Infrastructure": {
    "Persistence": {
      "Postgres": {
        "ConnectionString": "Host=ep-rapid-pine-a2fkl21y.eu-central-1.aws.neon.tech;Port=5432;Database=tickets_booking;Username=onulonus;Password=IzlciBL97Xew"
      }
    }
  },
  "Presentation": {
  },
  "Platform": {
    "Environment": ""
  },
  "Sentry": {
    "Enabled": true,
    "Environment": "",
    "Dsn": "",
    "Debug": false,
    "TracesSampleRate": 1.0,
    "MinimumEventLevel": "Warning"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://*:8080",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Extensions.Http": "Information",
        "Grpc.AspNetCore.Server": "Information",
        "Grpc.Net.Client": "Information",
        "System.Net.Http.HttpClient": "Information",
        "Npgsql.Command": "Warning",
        "Npgsql.Connection": "Warning",
        "Npgsql.Transaction": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:T} [{Level:u3}] {SourceContext} {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs/TicketsBooking/AppLogs_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] {SourceContext} {Message}{NewLine}{Exception}",
          "retainedFileCountLimit": 30
        }
      }
    ]
  }
}
